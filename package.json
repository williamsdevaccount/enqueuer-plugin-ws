{
  "name": "enqueuer-plugin-ws",
  "version": "0.0.1",
  "description": "Enqueuer web socket plugin",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "all": "npm run clean && npm run lint && npm run build",
    "build": "tsc",
    "clean": "rm -rf dist",
    "socketServerStart": "pm2 start socketserver/index.js --name \"socketserver\"",
    "socketServerStop": "pm2 stop socketserver && pm2 delete socketserver",
    "lint": "node_modules/.bin/tslint --project tsconfig.json",
    "prepublishOnly": "npm run all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/williamsdevaccount/enqueuer-plugin-ws"
  },
  "keywords": [
    "enqueuer",
    "nqr",
    "ws",
    "plugin"
  ],
  "author": "William Stowers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/williamsdevaccount/enqueuer-plugin-ws/issues"
  },
  "homepage": "https://github.com/williamsdevaccount/enqueuer-plugin-ws#readme",
  "devDependencies": {
    "@types/node": "^11.9.4",
    "@types/ws": "^6.0.1",
    "ts-node": "^8.0.2",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "enqueuer-plugins-template": "^1.0.4",
    "mqtt": "^2.18.8",
    "ws": "^6.1.4"
  }
}
